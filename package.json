{
  "name": "sfs-deploy-hub",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "tsx watch server/src/index.ts",
    "client": "vite",
    "build": "vite build",
    "health": "curl -f http://localhost:5000/health || exit 1",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/cors": "^2.8.15",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "vite": "^5.0.8",
    "concurrently": "^8.2.2"
  }
}
